<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="yobel_product_sequence" model="ir.sequence">
            <field name="name">Yobel Product</field>
            <field name="code">product.template</field>
            <field name="prefix">PRB%(year)s%(month)s%(day)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="yobel_embarque_sequence" model="ir.sequence">
            <field name="name">Yobel Embarque</field>
            <field name="code">stock.picking</field>
            <field name="prefix">EMB-PRB-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>