<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="yobel_master1_seq" model="ir.sequence">
            <field name="name">Yobel SCM 1</field>
            <field name="code">yobel_master_prb</field>
            <field name="prefix">PRB%(year)s%(month)s%(day)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="yobel_master2_seq" model="ir.sequence">
            <field name="name">Yobel SCM 2</field>
            <field name="code">yobel_master_emb</field>
            <field name="prefix">EMB-PRB-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>